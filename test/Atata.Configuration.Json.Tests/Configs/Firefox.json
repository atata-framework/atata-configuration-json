{
  "driver": {
    "type": "firefox",
    "options": {
      "additionalOptions": {
        "globalcap1": true,
        "globalcap2": 5,
        "globalcap3": "str"
      },
      "additionalBrowserOptions": {
        "cap1": false,
        "cap2": 15,
        "cap3": "str2"
      },
      "proxy": {
        "kind": "ProxyAutoConfigure"
      },
      "arguments": [ "--start-maximized" ],
      //"profile": {
      //  "profileDirectory": "dir",
      //  "deleteSourceOnClean": true,
      //  "enableNativeEvents": true,
      //  "preferences": {
      //    "pref1": true,
      //    "pref2": 5,
      //    "pref3": "str"
      //  }
      //},
      "preferences": {
        "pref1": true,
        "pref2": 5,
        "pref3": "str"
      },
      "logLevel": "warn"
    },
    "service": {
      "host": "127.0.0.5"
    },
    "commandTimeout": 0.95
  }
}
