{
  "driver": {
    "type": "chrome",
    "options": {
      "loggingPreferences": {
        "browser": "Info",
        "driver": "Warning"
      },
      "additionalCapabilities": {
        "cap1": true,
        "cap2": 5,
        "cap3": "str"
      },
      "globalAdditionalCapabilities": {
        "globalcap1": true,
        "globalcap2": 5,
        "globalcap3": "str"
      },
      "proxy": {
        "httpProxy": "http",
        "ftpProxy": "ftp"
      },
      "arguments": [ "disable-extensions", "no-sandbox", "start-maximized" ],
      "excludedArguments": [ "exc-arg" ],
      "encodedExtensions": [ "ZW5jLWV4dDE=", "ZW5jLWV4dDI=" ],
      "windowTypes": [ "win1", "win2" ],
      "performanceLoggingPreferences": {
        "isCollectingNetworkEvents": false,
        "IsCollectingPageEvents": false,
        "bufferUsageReportingInterval": "00:01:10",
        "tracingCategories": [ "cat1", "cat2" ]
      },
      "userProfilePreferences": {
        "pref1": 7,
        "pref2": false,
        "pref3": "str"
      },
      "localStatePreferences": {
        "pref1": 2.7,
        "pref2": true,
        "pref3": ""
      },
      "mobileEmulationDeviceName": "emul",
      "LeaveBrowserRunning": true,
      "minidumpPath": "mdp"
    },
    "service": {
      "port": 555,
      "whitelistedIPAddresses":  "5.5.5.5,7.7.7.7"
    }
  }
}
