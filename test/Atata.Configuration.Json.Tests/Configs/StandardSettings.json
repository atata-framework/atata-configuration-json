{
  "driver": {
    "type": "chrome",
    "options": {
      "arguments": [ "headless" ]
    }
  },
  "driverInitializationStage": "OnDemand",
  "baseUrl": "https://demo.atata.io/",
  "artifactsPathTemplate": "{test-suite-name-sanitized:/*}{test-name-sanitized:/prefix_*_postfix}",

  "variables": {
    "customIntVar": 7,
    "customStringVar": "strvar"
  },

  "domTestIdAttributeName": "data-autoid",
  "domTestIdAttributeDefaultCase": "midSentence",

  "useAllNUnitFeatures": true
}
